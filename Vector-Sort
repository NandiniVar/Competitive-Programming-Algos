/* 
Problem statement: sort a vector
Input: 5
1 6 10 8 4
Output: 1 4 6 8 10
*/


#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    cin>>n;
    vector<int>v;
    for(int i=0;i<n;i++)
    {   int x;
        cin>>x;
        v.push_back(x);}
    sort(v.begin(),v.end());
    for(int i=0;i<v.size();i++)
    cout<<v[i]<<" ";
    return 0;
}
